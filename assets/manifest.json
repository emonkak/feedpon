{
    "manifest_version": 3,
    "name": "Feedpon",
    "version": "2.1.1",
    "description": "A LDR inspired feed reader",
    "icons": {
        "16": "img/icon-16.png",
        "32": "img/icon-32.png",
        "48": "img/icon-48.png",
        "96": "img/icon-96.png",
        "128": "img/icon-128.png"
    },
    "action": {
        "default_icon": {
            "16": "img/icon-16.png",
            "24": "img/icon-24.png",
            "32": "img/icon-32.png"
        }
    },
    "background": {
        "service_worker": "background.js"
    },
    "host_permissions": [
        "<all_urls>"
    ],
    "permissions": [
        "tabs",
        "unlimitedStorage"
    ]
}
