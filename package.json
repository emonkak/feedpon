{
  "name": "feedpon",
  "version": "1.0.6",
  "description": "A feed reader which was developed for me",
  "author": "Shota Nozaki",
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/emonkak/feedpon.git"
  },
  "scripts": {
    "build": "npm-run-all build:dir -p build:css build:js -p build:cordova build:webextension -s build:zip",
    "build:cordova": "cp build/{index,runtime}.js www/js && cp build/index.css www/css && cordova prepare",
    "build:css": "postcss -o build/index.css res/css/index.css",
    "build:dir": "mkdir -p build dist www/{css,js} webextension/{css,js}",
    "build:js": "webpack",
    "build:webextension": "cp build/{index,background}.js webextension/js && cp build/index.css webextension/css",
    "build:zip": "cd webextension && zip -FSr ../dist/feedpon.zip .",
    "clean": "rm -f -r build dist webextension/{css,js} www/{css,js}",
    "lint": "tslint 'src/**/*.ts'",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "npm-run-all build:dir -p 'watch:*'",
    "watch:cordova": "watch 'npm run --silent build:cordova' build",
    "watch:css": "postcss -w -o build/index.css res/css/index.css",
    "watch:js": "webpack --watch --devtool source-map",
    "watch:webextension": "watch 'npm run --silent build:webextension' build"
  },
  "dependencies": {
    "@emonkak/enumerable": "^2.0.0",
    "classnames": "^2.2.5",
    "cordova-android": "^6.3.0",
    "cordova-ios": "^4.5.1",
    "cordova-plugin-inappbrowser": "^1.7.1",
    "cordova-plugin-statusbar": "^2.2.3",
    "cordova-plugin-whitelist": "^1.3.2",
    "fastclick": "^1.0.6",
    "fbjs": "^0.8.16",
    "intl": "^1.2.5",
    "intl-relativeformat": "^2.0.0",
    "lodash.debounce": "^4.0.8",
    "lodash.throttle": "^4.1.1",
    "object.values": "^1.0.4",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-addons-css-transition-group": "^15.6.2",
    "react-dom": "^16.0.0",
    "react-router": "^3.2.0",
    "redux-logger": "^3.0.6",
    "reselect": "^3.0.1",
    "text-encoding": "^0.6.4",
    "tslib": "^1.7.1",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "@types/chrome": "0.0.49",
    "@types/classnames": "2.2.3",
    "@types/cordova": "0.0.34",
    "@types/cordova-plugin-inappbrowser": "^1.7.0",
    "@types/lodash.debounce": "^4.0.3",
    "@types/lodash.throttle": "^4.1.3",
    "@types/prop-types": "^15.5.2",
    "@types/query-string": "^5.0.0",
    "@types/react": "^16.0.7",
    "@types/react-addons-css-transition-group": "^15.0.1",
    "@types/react-dom": "^15.5.5",
    "@types/react-router": "^3.0.13",
    "@types/redux-logger": "^3.0.4",
    "@types/text-encoding": "0.0.32",
    "autoprefixer": "^7.1.4",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "cash-cp": "^0.2.0",
    "cash-mkdir": "^0.2.0",
    "cash-rm": "^0.2.0",
    "npm-run-all": "^4.1.1",
    "postcss-cli": "^4.1.1",
    "postcss-color-function": "^4.0.0",
    "postcss-csso": "^2.0.0",
    "postcss-custom-media": "^6.0.0",
    "postcss-import": "^11.0.0",
    "postcss-simple-vars": "^4.1.0",
    "ts-loader": "^2.3.7",
    "tslint": "^5.7.0",
    "typescript": "^2.5.3",
    "watch": "^1.0.2",
    "webpack": "^3.6.0"
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {},
      "cordova-plugin-statusbar": {},
      "cordova-plugin-inappbrowser": {}
    },
    "platforms": [
      "android",
      "ios"
    ]
  }
}
